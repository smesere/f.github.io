{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter",
          "-webkit-text-size-adjust",
          "@-ms-viewport",
          "-webkit-overflow-scrolling",
          "@-ms-viewport"
        ]
      }
    ]
  }
}